<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.sleepingdragon.joko4nen.nosmoke" >

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".team_create.Team_createActivity"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:label="@string/app_name"
            android:theme="@style/FullscreenTheme" >

        </activity>
        <activity
            android:name=".regist"
            android:label="nosmoke" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".teamsetting.TeamSettingActivity"
            android:label="teamsetting" >
        </activity>
        <activity
            android:name=".team_invite.TeamInviteActivity"
            android:label="team_invite" >
        </activity>
    </application>

</manifest>
