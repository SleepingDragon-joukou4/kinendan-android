<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:background="#ffffff"
    android:id="@+id/body">

    <!-- The primary full-screen view. This can be replaced with whatever view
         is needed to present your content, e.g. VideoView, SurfaceView,
         TextureView, etc. -->

    <!-- This FrameLayout insets its children based on system windows using
         android:fitsSystemWindows. -->

    <LinearLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#b0c4de"
        android:id="@+id/schedule">


        <Spinner
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/schelist"
            android:layout_gravity="center_horizontal"
            android:layout_marginTop="10dp"
            android:layout_marginLeft="5dp"
            android:layout_marginRight="5dp"
            android:spinnerMode="dialog" />

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal"
            android:id="@+id/lay1">

            <TextView
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:textSize="22sp"
                android:gravity="center"
                android:textColor="#000"
                android:id="@+id/rank_rank"
                android:layout_marginLeft="30dp" />
            <TextView
                android:layout_width="150dp"
                android:layout_height="50dp"
                android:text="目標本数"
                android:textSize="22sp"
                android:gravity="center"
                android:textColor="#000"
                android:id="@+id/sche_honsu"
                android:layout_marginLeft="50dp" />
            <TextView
                android:layout_width="70dp"
                android:layout_height="50dp"
                android:text="実績"
                android:textSize="22sp"
                android:gravity="center"
                android:textColor="#000"
                android:id="@+id/sche_zisseki" />


        </LinearLayout>

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal"
            android:id="@+id/lay2">

            <TextView
                android:layout_width="130dp"
                android:layout_height="50dp"
                android:text="チーム全員"
                android:textSize="22sp"
                android:gravity="center"
                android:textColor="#000"
                android:id="@+id/sche_team" />
            <TextView
                android:layout_width="150dp"
                android:layout_height="50dp"
                android:text="50"
                android:textSize="22sp"
                android:gravity="center"
                android:textColor="#000"
                android:id="@+id/sche_teamhonsu" />
            <TextView
                android:layout_width="70dp"
                android:layout_height="50dp"
                android:text="45"
                android:textSize="22sp"
                android:gravity="center"
                android:textColor="#000"
                android:id="@+id/sche_teamzisseki" />


        </LinearLayout>

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal"
            android:id="@+id/lay3">

            <TextView
                android:layout_width="130dp"
                android:layout_height="50dp"
                android:text="A"
                android:textSize="22sp"
                android:gravity="center"
                android:textColor="#000"
                android:id="@+id/sche_team1" />
            <TextView
                android:layout_width="150dp"
                android:layout_height="50dp"
                android:text="6"
                android:textSize="22sp"
                android:gravity="center"
                android:textColor="#000"
                android:id="@+id/sche_honsu1" />
            <TextView
                android:layout_width="70dp"
                android:layout_height="50dp"
                android:text="3"
                android:textSize="22sp"
                android:gravity="center"
                android:textColor="#000"
                android:id="@+id/sche_zisseki1" />


        </LinearLayout>

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal"
            android:id="@+id/lay4">

            <TextView
                android:layout_width="130dp"
                android:layout_height="50dp"
                android:text="B"
                android:textSize="22sp"
                android:gravity="center"
                android:textColor="#000"
                android:id="@+id/sche_team2" />
            <TextView
                android:layout_width="150dp"
                android:layout_height="50dp"
                android:text="10"
                android:textSize="22sp"
                android:gravity="center"
                android:textColor="#000"
                android:id="@+id/sche_honsu2" />
            <TextView
                android:layout_width="70dp"
                android:layout_height="50dp"
                android:text="8"
                android:textSize="22sp"
                android:gravity="center"
                android:textColor="#000"
                android:id="@+id/sche_zisseki2" />


        </LinearLayout>

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal"
            android:id="@+id/lay5">

            <TextView
                android:layout_width="130dp"
                android:layout_height="50dp"
                android:text="C"
                android:textSize="22sp"
                android:gravity="center"
                android:textColor="#000"
                android:id="@+id/sche_team3" />
            <TextView
                android:layout_width="150dp"
                android:layout_height="50dp"
                android:text="25"
                android:textSize="22sp"
                android:gravity="center"
                android:textColor="#000"
                android:id="@+id/sche_honsu3" />
            <TextView
                android:layout_width="70dp"
                android:layout_height="50dp"
                android:text="26"
                android:textSize="22sp"
                android:gravity="center"
                android:textColor="#000"
                android:id="@+id/sche_zisseki3" />


        </LinearLayout>

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal"
            android:id="@+id/lay6">

            <TextView
                android:layout_width="130dp"
                android:layout_height="50dp"
                android:text="D"
                android:textSize="22sp"
                android:gravity="center"
                android:textColor="#000"
                android:id="@+id/sche_team4nk" />
            <TextView
                android:layout_width="150dp"
                android:layout_height="50dp"
                android:text="9"
                android:textSize="22sp"
                android:gravity="center"
                android:textColor="#000"
                android:id="@+id/sche_honsu4" />
            <TextView
                android:layout_width="70dp"
                android:layout_height="50dp"
                android:text="8"
                android:textSize="22sp"
                android:gravity="center"
                android:textColor="#000"
                android:id="@+id/sche_zisseki4" />


        </LinearLayout>

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal"
            android:gravity="center"
            android:id="@+id/lay7">

            <Button
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/sche_back"
                android:layout_marginRight="15dp"
                android:text="前" />
            <Button
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="次"
                android:id="@+id/sche_next"
                android:layout_marginLeft="15dp" />
        </LinearLayout>

    </LinearLayout>
    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal|bottom"
        android:id="@+id/lay8"
        android:layout_marginBottom="5dp">

        <Button
            android:layout_width="90dp"
            android:layout_height="90dp"
            android:text="ホーム"
            android:id="@+id/HomeButton"
            android:layout_gravity="center_horizontal" />
        <Button
            android:layout_width="90dp"
            android:layout_height="90dp"
            android:text="ランキング"
            android:id="@+id/RankingButton"
            android:layout_gravity="center_horizontal" />
        <Button
            android:layout_width="90dp"
            android:layout_height="90dp"
            android:text="スケジュール"
            android:id="@+id/ScheduleButton"
            android:layout_gravity="center_horizontal" />
        <Button
            android:layout_width="90dp"
            android:layout_height="90dp"
            android:text="商品表示"
            android:id="@+id/SyohinButton"
            android:layout_gravity="center_horizontal" />

    </LinearLayout>




    </FrameLayout>
